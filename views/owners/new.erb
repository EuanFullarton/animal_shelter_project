<form method="post" action="/owners">
  <label for="name">Name: </label>
  <input id="name" type="text" name="name"/>
  <br/>
  <br/>
  <label for="bio">Bio: </label>
  <input id="bio" type="text" name="bio">
  <br/>
  <br/>
  <label for="capacity_for_animals">Capacity for animals: </label>
  <input id="capacity_for_animals" type="number" name="capacity_for_animals"/>
  <br/>
  <br/>
  <label for="looking_for">Looking for... </label>
  <br/>
  <br/>
  <input id="looking_for1" type="checkbox" name="looking_for1" value = "Dog"/> Dog
  <input id="looking_for2" type="checkbox" name="looking_for2" value = "Cat"/> Cat
  <br/>
  <br/>
  <label for="profile_picture">Profile picture link: </label>
  <input id="profile_picture" type="text" name="profile_picture">
  <br/>
  <br/>
  <input type="submit" value="Register Owner"/>
</form>

